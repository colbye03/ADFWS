			"name": "colbyadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"Move input to output_properties_p_CopyEmployeeData_parameters_pl_input_container": {
						"value": "[parameters('Move input to output_properties_p_CopyEmployeeData_parameters_pl_input_container')]"
					},
					"Move input to output_properties_p_CopyEmployeeData_parameters_pl_input_path": {
						"value": "[parameters('Move input to output_properties_p_CopyEmployeeData_parameters_pl_input_path')]"
					},
					"Move input to output_properties_p_CopyEmployeeData_parameters_pl_input_filename": {
						"value": "[parameters('Move input to output_properties_p_CopyEmployeeData_parameters_pl_input_filename')]"
					},
					"Move input to output_properties_p_CopyEmployeeData_parameters_pl_output_container": {
						"value": "[parameters('Move input to output_properties_p_CopyEmployeeData_parameters_pl_output_container')]"
					},
					"Move input to output_properties_p_CopyEmployeeData_parameters_pl_output_path": {
						"value": "[parameters('Move input to output_properties_p_CopyEmployeeData_parameters_pl_output_path')]"
					},
					"Move input to output_properties_p_CopyEmployeeData_parameters_pl_output_filename": {
						"value": "[parameters('Move input to output_properties_p_CopyEmployeeData_parameters_pl_output_filename')]"
					},
					"Move input to output_properties_typeProperties_scope": {
						"value": "[parameters('Move input to output_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}